{
  "extends": ["react-app", "plugin:cypress/recommended"],
  "rules": {
    "curly": ["error"],
    "valid-typeof": ["error"],
    "camelcase": "error",
    "id-length": ["error", { "min": 3, "exceptions": ["_","a","b","d","e","i","j","k","x","y","id","el","pi","PI","up"] }],
    "no-var": ["error"],
    "lines-between-class-members": ["error", "always"]
  }
}

